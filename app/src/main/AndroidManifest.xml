<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lovelyhearts.poker" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EditaccountActivity"
            android:label="@string/title_activity_editaccount" >
        </activity>
        <activity
            android:name=".CreateaccountActivity"
            android:label="@string/title_activity_createaccount" >
        </activity>
        <activity
            android:name=".DetailaccountActivity"
            android:label="@string/title_activity_detailaccount" >
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_home" >
        </activity>
        <activity
            android:name=".LocationDetails"
            android:label="@string/title_activity_location_details" >
        </activity>
        <activity
            android:name=".EditLocation"
            android:label="@string/title_activity_edit_location" >
        </activity>
        <activity
            android:name=".EditTournament"
            android:label="@string/title_activity_edit_tournament" >
        </activity>
        <activity
            android:name=".TournamentTime"
            android:label="@string/title_activity_tournament" >
        </activity>
        <activity
            android:name=".TournamentLocation"
            android:label="@string/title_activity_tournament_location" >
        </activity>
        <activity
            android:name=".TournamentActivity"
            android:label="@string/title_activity_tournament" >
        </activity>
    </application>

</manifest>
